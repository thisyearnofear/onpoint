{
  "manifest_version": 3,
  "name": "OnPoint Fashion AI",
  "version": "1.0.0",
  "description": "AI-powered fashion critique and styling using Chrome Built-in AI APIs",
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "notifications",
    "alarms",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "OnPoint Fashion AI"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "popup.html",
        "styles/*",
        "scripts/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "host_permissions": [
    "http://localhost:3000/*",
    "https://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
